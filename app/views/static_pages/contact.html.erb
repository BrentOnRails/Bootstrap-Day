<h1>Let's Connect</h1>

<div class="row">
  <div class="col-sm-6">
    <form>
      <div class="form-group">
        <label for="contact-name">Name</label>
        <input id="contact-name" type="text" class="form-control" placeholder="Your name">
      </div>

      <div class="form-group">
        <label for="contact-email">Email</label>
        <input id="contact-email" type="text" class="form-control" placeholder="Your email">
      </div>

      <div class="form-group">
        <label for="contact-phone">Phone</label>
        <input id="contact-phone" type="text" class="form-control" placeholder="Your phone">
      </div>

      <div class="form-group">
        <label for="contact-body">Message</label>
        <textarea id="contact-body" class="form-control" placeholder="What do you want to know?"></textarea>
      </div>

      <button type="button" class="btn btn-primary" id="contact-submit">Submit</button>
    </form>
  </div>

